{
  "name": "templates",
  "projectType": "library",
  "sourceRoot": "packages/uimatrix/templates/src",
  "prefix": "hyperconverged",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/uimatrix/templates"],
      "options": {
        "jestConfig": "packages/uimatrix/templates/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/uimatrix/templates/src/**/*.ts",
          "packages/uimatrix/templates/src/**/*.html"
        ]
      },
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": []
}
