{
  "name": "templates",
  "projectType": "library",
  "sourceRoot": "packages/uimatrix/ui-templates/src",
  "prefix": "ui",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/packages/uimatrix/ui-templates"
      ],
      "options": {
        "jestConfig": "packages/uimatrix/ui-templates/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/uimatrix/ui-templates/src/**/*.ts",
          "packages/uimatrix/ui-templates/src/**/*.html"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    }
  },
  "tags": []
}
